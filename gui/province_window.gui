				text = "[ProvinceWindow.GetProvince.GetAreaName]"  
				margin_left = @window_margin_left_title
			}
			blockoverride "White_buttons"
			{
				button_close = { 
					visible = yes
					onclick = "[ProvinceWindow.OnClose]"
					tooltip = "CLOSE"
				}
			}
		}
		
