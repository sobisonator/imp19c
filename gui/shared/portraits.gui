		size = { 244 344 }
		scale = 0.15
		alwaystransparent = yes
		visible = [Character.IsImprisoned]
		texture = "gfx/portraits/portrait_frame_imprisonment.dds"
	}
	
	icon = {
		position = { 0 5 }
		visible = [Character.IsDeadAndValid]
		using = icon_attrition
		size = { 20 20 }
		tooltip = CHARACTER_DEAD_TOOLTIP
	}
}

