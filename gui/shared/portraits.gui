	framesize = { 86 129 }
	frame_tier = [Character.GetPortraitFrameFrame]
	
	icon = {
		name = "player_overlay"
		position = { 20 20 }
		parentanchor = center
		size = { 0 0 }
		alwaystransparent = yes
		texture = gfx/portraits/portrait_player_glow.dds
		visible = [Character.IsOtherPlayerRuler]
	}
	icon = {
		name = "imprisonment"
		position = { 0 0 }
